{
  "name": "craft-ai",
  "version": "1.2.2",
  "description": "craft ai API isomorphic (compatible with browser and nodejs) javascript client",
  "author": {
    "name": "craft ai",
    "email": "contact@craft.ai",
    "url": "http://craft.ai/"
  },
  "homepage": "https://github.com/craft-ai/craft-ai-client-js",
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/craft-ai/craft-ai-client-js/issues"
  },
  "main": "lib/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/craft-ai/craft-ai-client-js"
  },
  "browser": {
    "lodash": "lodash/lodash.min.js"
  },
  "keywords": [
    "ai",
    "craft-ai"
  ],
  "scripts": {
    "update_readme": "download http://www.craft.ai/content/api/js.md > README.md && git add README.md && git commit -m 'Updated README file'",
    "build": "npm run build_node && npm run build_browser:production && npm run build_browser:dev",
    "build_node": "babel src --out-dir lib",
    "build_browser:production": "cross-env NODE_ENV=production webpack",
    "build_browser:dev": "cross-env NODE_ENV=development webpack",
    "lint": "eslint .",
    "fix_lint": "eslint --fix .",
    "test": "npm run test_node && npm run lint",
    "test_node": "mocha",
    "dev_browser": "cd test/browser && webpack-dev-server",
    "prepublish": "npm run build",
    "publish_bundle": "./gh-deploy.sh"
  },
  "devDependencies": {
    "babel-cli": "6.24.1",
    "babel-core": "6.24.1",
    "babel-eslint": "6.1.2",
    "babel-loader": "6.4.1",
    "babel-polyfill": "6.23.0",
    "babel-preset-env": "1.4.0",
    "babel-register": "6.24.1",
    "chai": "3.5.0",
    "cross-env": "4.0.0",
    "dotenv": "4.0.0",
    "download-cli": "1.0.1",
    "eslint": "2.13.1",
    "eslint-config-craft-ai": "2.2.0",
    "mocha": "3.2.0",
    "mocha-loader": "1.1.1",
    "webpack": "2.4.1",
    "webpack-dev-server": "2.4.2"
  },
  "dependencies": {
    "debug": "2.6.3",
    "inherits": "2.0.3",
    "jwt-decode": "2.2.0",
    "lodash": "4.17.4",
    "moment": "2.18.1",
    "node-fetch": "1.6.3",
    "semver": "5.3.0",
    "whatwg-fetch": "2.0.3"
  }
}
